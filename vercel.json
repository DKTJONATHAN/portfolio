 {
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    },
    {
      "source": "/:path*",
      "destination": "/:path*/index.html"
    },
    {
      "source": "/sitemap",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots",
      "destination": "/robots.txt"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}