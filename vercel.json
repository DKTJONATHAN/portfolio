{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/sitemap",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots",
      "destination": "/robots.txt"
    }
  ]
}