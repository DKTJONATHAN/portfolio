{
  "routes": [
    {
      "src": "/",
      "dest": "/index.html",
      "headers": {
        "X-Vercel-Route": "Root"
      }
    },
    {
      "src": "/admin",
      "dest": "/admin/index.html",
      "headers": {
        "X-Vercel-Route": "Admin"
      }
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1",
      "headers": {
        "X-Vercel-Route": "Admin assets"
      }
    },
    {
      "src": "/articles/([^/.]+)",
      "dest": "/content/articles/$1.html",
      "headers": {
        "X-Vercel-Route": "Articles"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "X-Vercel-Route": "API"
      }
    },
    {
      "src": "/content/articles.json",
      "dest": "/content/articles.json",
      "headers": {
        "Cache-Control": "public, max-age=3600",
        "Access-Control-Allow-Origin": "*",
        "X-Vercel-Route": "Articles JSON"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000",
        "X-Vercel-Route": "Images"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "X-Vercel-Route": "Static file catch-all"
      }
    }
  ]
}