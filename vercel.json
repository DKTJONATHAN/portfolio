{
  "version": 2,
  "routes": [
    {
      "src": "/api/fetch-posts",
      "dest": "/api/fetch-posts.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/save-post",
      "dest": "/api/save-post.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}