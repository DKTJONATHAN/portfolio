{
  "routes": [
    {
      "src": "/articles/([^/.]+)",
      "dest": "/content/articles/$1.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/content/articles.json",
      "dest": "/content/articles.json",
      "headers": {
        "Cache-Control": "public, max-age=3600",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}