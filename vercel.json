{
  "version": 2,
  "routes": [
    {
      "src": "/api/fetch-posts",
      "dest": "/api/fetch-posts.js",
      "timeout": 30
    },
    {
      "src": "/api/save-post",
      "dest": "/api/save-post.js",
      "timeout": 60
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js",
      "timeout": 10
    },
    {
      "src": "/images/logo.png",
      "dest": "/images/Jonathan-Mwaniki-logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}